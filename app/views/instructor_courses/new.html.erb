<div>
  <center>

  <h1>Assign Instructors to Courses</h1>
      <%= form_for(:instructor_course,:url =>{:action => 'create'}) do |f| %>
        <div class="field">
          <%= f.label :course_id %><br>
          <%= f.text_field :course_id %>
        </div>
        <div class="field">
          <%= f.label :email %><br>
          <%= f.text_field :email %>
        </div>

        <div class="actions">
          <%= f.submit "Create"%>
        </div>
      <% end %>

  </center>
</div>
<div>
  <nav class="navbar navbar-default">
    <button type="button" class="btn btn-default navbar-btn"><%= link_to('Back', :controller => 'admins' , :action => 'home')%></button>
    </nav>
</div>