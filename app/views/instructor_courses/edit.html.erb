<div>
  <center>
    <h1>Edit Instructors Assigned To Courses</h1>
<% @instructor_course = InstructorCourse.find_by_id(params[:id]) %>
<%= form_for @instructor_course, url: {:action=>'update' } do |f| %>
        <div class="field">
          <%= f.label :course_id %><br>
          <%= f.text_field :course_id %>
        </div>
        <div class="field">
          <%= f.label :email %><br>
          <%= f.text_field :email %>
        </div>
        <div class="actions">
          <%= f.submit "Save Changes"%>
        </div>
<% end %>
  </center>
</div>
<nav class="navbar navbar-default">
  <%=link_to({:controller => 'instructor_courses' , :action => 'index'}, {:class => 'btn btn-default'}) do%>
      <i>Back</i><%end%>
</nav>