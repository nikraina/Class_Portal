<h1>Instructor Course Enrollment</h1>
<%= form_for(:instructor_course,:url =>{:action => 'create'}) do |f| %>
    <div class="field">
      <%= f.label :course_id %><br>
      <%= f.text_field :course_id %>
    </div>
    <div class="field">
      <%= f.label :email %><br>
      <%= f.text_field :email %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
<%= link_to('Back', :controller => 'admin' , :action => 'index')%>