<h1>Instructor Course Profile </h1>
<table>
  <thead>
  <tr>
    <th>Course ID</th>
    <th>Instructor Email</th>
  </tr>
  </thead>
  <% @user=User.find(session[:id]) %>
  <tbody>
  <% @instructor_course=InstructorCourse.find(params[:id]) %>

  <tr>
    <td><%= @instructor_course.course_id %><br></td>
    <td><%= @instructor_course.email %><br></td>

  </tr>
  </tbody>
</table>
<%= link_to('Edit', :controller => 'instructor_courses' , :action => 'edit')%>
<% if @user.is_admin %>
    <%= link_to('Back to Display All users', :controller => 'admin' , :action => 'show')%>
    <%= link_to('Homepage', :controller => 'admin' , :action => 'index')%>
<% else%>
    <%= link_to('Back', :controller => 'instructor' , :action => 'index')%>
<%end%>